e>\r\n\r\n<script>\r\nconst focus = {\r\n  inserted: (el) => {\r\n    el.focus();\r\n  },\r\n};\r\n\r\nexport default {\r\n  directives: {\r\n    focus,\r\n  },\r\n  data: () => ({\r\n    showList: false,\r\n    currentTask:'' ,\r\n    tasks: [{ name: \"Fazer o Curso\", isDone: false }],\r\n  }),\r\n  methods: {\r\n    handleShowHideList() {\r\n      this.showList = !this.showList;\r\n    },\r\n    addTask (){\r\n      this.tasks.push({\r\n        name: this.currentTask,\r\n        isDone: false\r\n      })\r\n      this.currentTask = ''\r\n    },\r\n    complete(task) {\r\n      this.tasks = this.tasks.map((t) => {\r\n        if (t.name === task.name) {\r\n          return { ...t, isDone: !t.isDone };\r\n        }\r\n        return { ...t };\r\n      });\r\n    },\r\n    remove(task) {\r\n      this.tasks = this.tasks.filter((t) => t.name !== task.name);\r\n      console.log(\"task\", task);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.linha {\r\n  text-decoration: line-through;\r\n}\r\n</style> "]}]}